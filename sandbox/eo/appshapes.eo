+package sandbox

+alias printshape sandbox.printshape
+alias inputshapes sandbox.inputshapes
+alias stdout org.eolang.io.stdout
+alias sprintf org.eolang.txt.sprintf

[args...] > appshapes
  inputshapes 0 args > shapes
    each. > @
      shapes
        [f]
          stdout > @
            sprintf
              "%s\n"
              printshape f
